<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<classPathEntry location="mysql-connector-java-5.1.22-bin.jar" />

	<context id="yueba" targetRuntime="MyBatis3">
	
		<plugin type="org.mybatis.generator.plugins.AddLimitOffsetPlugin" />
		
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/myssh" userId="root" password="admin888">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="cn.ptp.entity"
			targetProject="../src/main/java/">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="cn.ptp.dao"
			targetProject="../src/main/java/">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
			targetPackage="cn.ptp.dao" targetProject="../src/main/java/">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>


		<!--
		<table schema="yueba" tableName="message" domainObjectName="Message" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
		<table schema="yueba" tableName="user" domainObjectName="User" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
		-->

		<table schema="yueba" tableName="message" domainObjectName="Message">
			<property name="useActualColumnNames" value="true" />
			<generatedKey column="id" sqlStatement="mysql" identity="true" />
		</table>
		<table schema="yueba" tableName="user" domainObjectName="User">
			<property name="useActualColumnNames" value="true" />
			<generatedKey column="id" sqlStatement="mysql" identity="true" />
		</table>
		<!-- 
		<table schema="yueba" tableName="message" domainObjectName="Message">
			<property name="useActualColumnNames" value="true" />
			<generatedKey column="id" sqlStatement="mysql" identity="true" />
			<columnOverride column="msg" jdbcType="VARCHAR" />
		</table>
		-->
	</context>
</generatorConfiguration>